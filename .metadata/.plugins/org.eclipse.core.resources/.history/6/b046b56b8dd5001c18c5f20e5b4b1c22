<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

<%
	int i=0;
	int j=0;
	int size=0;
%>
<%
	size++;
%>
<table border="1">
<%
	for(i=1;i<=size;i++){
%>
		<tr>
<%
		for(j=1;j<=size;j++){
%>
			<td><%=i%>*<%=j%>=<%=i*j%></td>
							
<%
		}
%>
		</tr>
<%
	}
%>
<%
	i++;
	j++;
%>	
</table>
</body>
</html>